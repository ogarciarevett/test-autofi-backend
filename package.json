{
  "name": "test-autofi-backend",
  "version": "1.0.0",
  "description": "Backend upload test - Omar Garcia",
  "main": "dist/index.js",
  "scripts": {
    "test-ci": "npm run lint && npm run compile && npm run mocha",
    "test": "ts-mocha \"tests/**/*.ts\" --recursive --exit --timeout 9999999",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev --rs --no-notify -r dotenv/config src/",
    "format": "prettier --write src/*.ts src/**/*.ts",
    "build": "npx rimraf rm -rf lib/ && tsc",
    "start": "node lib/index.js"
  },
  "engines": {
    "node": "15.10.x",
    "npm": "7.17.x"
  },
  "author": "Omar Garcia",
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.18",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/express-fileupload": "^1.1.6",
    "@types/http-errors": "^1.8.0",
    "@types/lodash": "^4.14.170",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.5",
    "@types/signale": "^1.4.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^8.4.0",
    "mongodb-memory-server": "^6.9.6",
    "prettier": "^2.3.1",
    "ts-mocha": "^8.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@typegoose/typegoose": "^7.4.2",
    "body-parser": "^1.19.0",
    "chai": "^4.3.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv": "^5.5.0",
    "csv-parser": "^3.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "fast-csv": "^4.3.6",
    "helmet": "^4.6.0",
    "http-errors": "^1.7.2",
    "http-status-codes": "^2.1.4",
    "lodash": "^4.17.21",
    "mongoose": "^5.10.18",
    "multer": "^1.4.2",
    "signale": "^1.4.0",
    "supertest": "^6.1.3"
  }
